											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: 'Thursday 3:00 pm - 4:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})

								div#tabs-1-2.tab-pane.fade
									+event-item-classic({
										img: 'user-1-109x109',
										eventTime: '1:00 pm - 3:00 pm ',
										title: 'Hip-Hop Techniques (Group Lesson)',
										speaker: 'Meryl McMillan'
									})
									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: '3:00 pm - 5:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.

									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '11:00 am - 1:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})
									+event-item-classic({
										eventColection: true,
										listImg: [
											{img: 'user-1-60x60'},
											{img: 'user-2-60x60'},
											{img: 'user-3-60x60'}
										],
										eventTime: '9:00 am - 11:00 am ',
										title: 'Contemporary Dance Workshop (Urban/Breakdance)',
										listSpeaker:  [
											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
								div#tabs-1-3.tab-pane.fade
									+event-item-classic({
										eventColection: true,
										listImg: [
											{img: 'user-1-60x60'},
											{img: 'user-2-60x60'},
											{img: 'user-3-60x60'}
										],
										eventTime: '9:00 am - 11:00 am ',
										title: 'Contemporary Dance Workshop (Urban/Breakdance)',
										listSpeaker:  [
											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: '1:00 pm - 3:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.
									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '11:00 am - 1:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})


								div#tabs-1-4.tab-pane.fade

									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '9:00 am - 11:00 am ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})

									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: 'Tuesday 11:00 am - 12:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.



	// Partners
	section.section.section-lg.bg-default.text-center
		.container
			.wow-outer
				.wow.slideInDown
					h3.title-decorate.title-decorate-center Partners
			.row.row-30
				.col-sm-6.col-lg-3
					a(href='#').box-sponsor.wow.fadeInUp
						+png('logo-1-200x200')
				.col-sm-6.col-lg-3
					a(href='#' data-wow-delay='0.3s').box-sponsor.wow.fadeInUp
						+png('logo-2-200x200')
				.col-sm-6.col-lg-3
					a(href='#' data-wow-delay='0.6s').box-sponsor.wow.fadeInUp
						+png('logo-3-200x200')
				.col-sm-6.col-lg-3
					a(href='#' data-wow-delay='0.9s').box-sponsor.wow.fadeInUp
						+png('logo-4-200x200')
	// Pricing
	section.section.section-xl.bg-accent-dark.text-center.decorated-section-3
		.container.wow.FadeIn
			h3.title-decorate.title-decorate-center Our Pricing Plans
			// Owl Carousel
			.row
				.col-12
					.owl-carousel(data-items='1', data-lg-items='3' data-dots='true' data-nav='false' data-stage-padding='0', data-loop='false', data-margin='0', data-mouse-drag="false")
						.box-pricing-classic

							h4.pricing-classic-title Basic
							ul.pricing-classic-list
								li 1 Personal Lesson
								li 3 Group Lessons
								li 5 Workshops
								li 3 Classes for Children
								li Free First Lesson
							h3.pricing-classic-price
								span $
								| 25
							a(href='#').button.button-lg.button-primary Buy Now
						.box-pricing-classic.box-pricing-selected.box-pricing-centered
							h4.pricing-classic-title Standard
							ul.pricing-classic-list
								li 3 Personal Lessons
								li 10 Group Lessons
								li 15 Workshops
								li 3 Classes for Teens
								li 2 Free Lessons
							h3.pricing-classic-price
								span $
								| 45
							a(href='#').button.button-lg.button-primary Buy Now
						.box-pricing-classic
							h4.pricing-classic-title Premium
							ul.pricing-classic-list
								li 5 Personal Lessons
								li 15 Group Lessons
								li 25 Workshops
								li 3 Classes for Adults
								li 3 Free Lessons
							h3.pricing-classic-price
								span $
								| 70
							a(href='#').button.button-lg.button-primary Buy Now





