		.row.isotope-wrap
			// Isotope Content
			.col-lg-12
				.isotope(data-isotope-layout='masonry', data-isotope-group='gallery' data-lightgallery='group' data-lg-thumbnail='false' )
					.row.no-gutters.row-condensed
						.col-12.col-sm-6.col-md-4.col-lg-3.isotope-item(data-filter='Category 2').wow-outer
							.wow.fadeInUp
								+gallery-item-classic({
									img: 'gallery-home-1-480x215',
