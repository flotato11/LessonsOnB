	section.section.text-center
		+parallax({
			img: 'parallax-img-1.jpg',
			innerClass: 'section-xl context-dark text-center bg-dark-filter'
		})
			.container: .row.justify-content-md-center: .col-md-10.wow-outer
				.wow.fadeInUp
					h3.title-decorate-center.title-decorate.font-weight-regular Take Your First Dance Lesson for Free
					a(href='#').button.button-xl.button-primary Book a lesson

	section.section.section-lg.bg-gray-1
		.container
			.row.row-50
				.col-12.text-center
					h3.title-decorate.title-decorate-center Latest News
				.col-md-6.col-lg-4.wow-outer
					.wow.fadeInUp
						+post-modern({
							img: 'post-modern-1-370x252',
							badgeMod: 'badge-primary',
							badge: 'Dance',
							date: 'May 11, 2018',
