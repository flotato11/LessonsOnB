extends  ../_skeleton

//- Includes
include ../components/_ui-pagination
include ../components/_ui-box-icon
include ../components/_ui-team
include ../components/_ui-posts
include ../components/_ui-gallery-item
include ../components/_ui-event-item

block variables
	- var activeCategory = 'home'
	- var pageName = 'Home'

block page
	// Page Header
	include ../sections/_header-modern

	// Swiper
	section.section.section-lg.section-main-bunner.bg-light-filter
		.main-bunner-img.decorated-section-5(style='background-image: url("images/bg-bunner-1.jpg"); background-size: cover;')
		.main-bunner-inner
			.container
				.row.row-50.justify-content-lg-between.align-items-lg-center
					.col-lg-9.col-xl-8.order-lg-1
						h5.bunner-location.d-flex.align-items-center
							span.icon.mdi.mdi-map-marker.icon-secondary
							a(href='#').text-opacity-80 july 05-09, 2018,
								br.d-sm-none
								|  Los angeles, ca
						h1 urban Dance Masterclass
						.countdown-primary
							.countdown(data-type='until', data-time='17 Dec 2018 16:00', data-format='dhms')

	// Box icon
	section.section.section-lg.bg-gray-1.decorated-section-1
		.container
			.row.row-50
				.col-md-6.col-lg-4.wow-outer
					.wow.slideInLeft
						+box-icon-classic({
							classMod: 'icon-vector-1',
							icon: 'linearicons-user icon-xl-color',
							title: 'Personal Training'
						})
							p Personal training offers one-on-one individually designed dance sessions that perfectly fit your needs.
				.col-md-6.col-lg-4.wow-outer
					.wow.slideInDown
						+box-icon-classic({
							classMod: 'icon-vector-2 ',
							icon: 'linearicons-group-work icon-xl-color',
							title: 'Group Training'
						})
							p Group training allows you to enjoy the best dance styles while learning new moves with other students.
				.col-md-6.col-lg-4.wow-outer
					.wow.slideInRight
						+box-icon-classic({
							classMod: 'icon-vector-3',
							icon: 'linearicons-loudspeaker icon-xl-color ',
							title: 'Dance Workshops'
						})
							p If you are not sure what to pick, you can always try one of our regular dance workshops.






	// Team
	section.section.section-xl
		.container
			.row.row-30.justify-content-center.text-center
				.col-12.wow-outer
					.wow.slideInDown
						h3.title-decorate.title-decorate-center Instructors
				.col-lg-8.wow-outer
					.wow.fadeInUp
						p Since our establishment, we have gathered a team of more than 20 dance instructors who have experience in various dance styles.
			.row.no-gutters
				.col-md-6.col-lg-4
					.wow.fadeInUp
						+team-classic({
							img: 'team-classic-1-391x252',
							name: 'Kate Anderson',
							text: 'Founder, Senior Instructor'
						})
				.col-md-6.col-lg-4
					.wow.fadeInUp

						+team-classic({
							img: 'team-classic-2-391x252',
							name: 'Jessica Peterson',
							text: 'Dance instructor, latin'
						})
				.col-md-6.col-lg-4
					.wow.fadeInUp
						+team-classic({
							img: 'team-classic-3-391x252',
							name: 'Richard Williams',
							text: 'Dance instructor, House'
						})
				.col-md-6.col-lg-4
					.wow.fadeInUp
						+team-classic({
							img: 'team-classic-4-391x252',
							name: 'Meryl McMillan',
							text: 'Dance instructor, Hip-Hop'
						})
				.col-md-6.col-lg-4
					.wow.fadeInUp
						+team-classic({
							img: 'team-classic-5-391x252',
							name: 'Henry Wilson',
							text: 'Dance instructor, Urban'
						})
				.col-md-6.col-lg-4
					.wow.fadeInUp
						+team-classic({
							img: 'team-classic-6-391x252',
							name: 'Stella Clark',
							text: 'Dance instructor, Breakdance'
						})


