extends  ../_skeleton

include ../components/_ui-event-item

block variables
	- var activeCategory = 'pages'
	- var pageName = 'Schedule #1'

block page 
	// Page Header
	include ../sections/_header-modern

	+breadcrumbs({
		category: activeCategory,
		path: [pageName],
		img: ''
	})
	
	section.section-lg.section.bg-gray-1
		.container
			#tabs-1.tabs-custom.tabs-horizontal.tabs-modern
				.row.no-gutters
					.col-lg-4.col-xl-3.order-lg-2.wow-outer
						.wow.slideInRight
							ul.nav.nav-tabs.nav-tabs-modern
								li.nav-item(role='presentation'): a.nav-link.active(href='#tabs-1-1' data-toggle='tab') Basic
								li.nav-item(role='presentation'): a.nav-link(href='#tabs-1-2' data-toggle='tab') Intermediate
								li.nav-item(role='presentation'): a.nav-link(href='#tabs-1-3' data-toggle='tab') Advanced
								li.nav-item(role='presentation'): a.nav-link(href='#tabs-1-4' data-toggle='tab') Professional
							a(href='#').button.button-lg.button-primary.button-tabs-modern Book a free lesson
					.col-lg-8.col-xl-9.order-lg-1.wow-outer
						.wow.slideInLeft
							.tab-content
								div#tabs-1-1.tab-pane.fade.show.active
									+event-item-classic({
										img: 'user-1-109x109',
										eventTime: 'Monday 9:00 am - 10:00 am ',
										title: 'Hip-Hop  Techniques (Group Lesson)',
										speaker: 'Meryl McMillan'
									})
									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: 'Tuesday 11:00 am - 12:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.
									+event-item-classic({
										eventColection: true,
										listImg: [
											{img: 'user-1-60x60'},
											{img: 'user-2-60x60'},
											{img: 'user-3-60x60'}
										],
										eventTime: 'Wednesday 1:00 pm - 2:00 pm',
										title: 'Contemporary Dance Workshop (Urban/Breakdance)',
										listSpeaker: [
											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: 'Thursday 3:00 pm - 4:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})

								div#tabs-1-2.tab-pane.fade
									+event-item-classic({
										img: 'user-1-109x109',
										eventTime: '1:00 pm - 3:00 pm ',
										title: 'Hip-Hop Techniques (Group Lesson)',
										speaker: 'Meryl McMillan'
									})
									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: '3:00 pm - 5:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.

									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '11:00 am - 1:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})
									+event-item-classic({
										eventColection: true,
										listImg: [
											{img: 'user-1-60x60'},
											{img: 'user-2-60x60'},
											{img: 'user-3-60x60'}
										],
										eventTime: '9:00 am - 11:00 am ',
										title: 'Contemporary Dance Workshop (Urban/Breakdance)',
										listSpeaker: [
											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
								div#tabs-1-3.tab-pane.fade
									+event-item-classic({
										eventColection: true,
										listImg: [
											{img: 'user-1-60x60'},
											{img: 'user-2-60x60'},
											{img: 'user-3-60x60'}
										],
										eventTime: '9:00 am - 11:00 am ',
										title: 'Contemporary Dance Workshop (Urban/Breakdance)',
										listSpeaker: [
											{speaker: 'M. McMillan,'},
											{speaker: 'H. Wilson,'},
											{speaker: 'K. Anderson'}
										]
									})
									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: '1:00 pm - 3:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.
									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '11:00 am - 1:00 pm ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})


								div#tabs-1-4.tab-pane.fade

									+event-item-classic({
										img: 'user-3-109x109',
										eventTime: '9:00 am - 11:00 am ',
										title: 'Personal House Dance Class',
										speaker: 'Richard Williams'
									})

									+event-item-classic({
										img: 'user-2-109x109',
										eventTime: 'Tuesday 11:00 am - 12:00 pm ',
										title: 'Urban Dance Class (Teens & Adults)',
										speaker: 'Henry Wilson',
										eventText: true,
										eventLocation: 'Contemp Training Hall'
									})
										p Learn the basics of Urban Dance during a series of classes conducted by our certified and award-winning teacher Henry Wilson.



	// Page Footer
	include ../sections/_footer-classic
